{
  "name": "alex-patterson-webdev/laminas-date-time",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laminas/laminas-mvc": "^3.1",
    "alex-patterson-webdev/date-time": "^1.1",
    "alex-patterson-webdev/laminas-factory": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasDateTime\\\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasDateTime\\": "test/phpunit"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit --coverage-clover=coverage.xml"
  },
  "config": {
    "secure-http": false,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
