{
  "name": "alex-patterson-webdev/laminas-date-time",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4 || >=8.0",
    "laminas/laminas-eventmanager" : "^3.3",
    "laminas/laminas-modulemanager" : "^2.10",
    "laminas/laminas-servicemanager" : "^3.0",
    "alex-patterson-webdev/date-time": "^2",
    "alex-patterson-webdev/laminas-factory": "dev-feature/3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laminas/laminas-view": "^2.12",
    "laminas/laminas-hydrator" : "^3.2"
  },
  "suggest": {
    "laminas/laminas-view" : "^2.12 to use the view helper date components",
    "laminas/laminas-hydrator" : "^3.2 to use the hydrator components"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasDateTime\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasDateTime\\": "test/phpunit/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit --coverage-clover=coverage.xml"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
